{
  "name": "react-anchorify-text",
  "version": "1.1.2",
  "description": "React component which render text with anchored url(s)",
  "main": "lib/index.js",
  "scripts": {
    "browser": "browser-sync start --files example/* --server example",
    "watch:example": "watchify example/app.js -dv -o example/bundle.js",
    "start:example": "npm run watch:example & npm run browser",
    "test:local": "karma start",
    "test": "./node_modules/.bin/karma start --browsers Firefox --single-run",
    "clean": "rimraf lib",
    "build": "babel src --out-dir lib"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/georgeosddev/react-anchorify-text"
  },
  "keywords": [
    "react",
    "react-component",
    "url",
    "anchor",
    "link"
  ],
  "author": "Takeharu.Oshida",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/georgeosddev/react-anchorify-text/issues"
  },
  "homepage": "https://github.com/georgeosddev/react-anchorify-text",
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-eslint": "^4.1.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "browser-sync": "^2.9.12",
    "browserify": "^12.0.1",
    "chai": "^3.4.1",
    "eslint": "^1.9.0",
    "eslint-plugin-react": "^3.8.0",
    "karma": "^0.13.15",
    "karma-browserify": "^4.4.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.1",
    "karma-cli": "0.1.1",
    "karma-firefox-launcher": "^0.1.6",
    "karma-mocha": "^0.2.0",
    "karma-safari-launcher": "^0.1.1",
    "karma-spec-reporter": "0.0.22",
    "mocha": "^2.3.3",
    "react-addons-test-utils": "^0.14.2",
    "rimraf": "^2.5.0",
    "watchify": "^3.6.0"
  },
  "dependencies": {
    "react": "^0.14.2",
    "react-dom": "^0.14.2"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "compact": false,
          "presets": [
            "es2015",
            "react"
          ]
        }
      ]
    ]
  }
}
